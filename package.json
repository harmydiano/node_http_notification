{
    "name": "pangaea-test",
    "version": "1.0.0",
    "description": "A simple http notification system",
    "main": "app.ja",
    "engines": {
        "node": "10.4.1",
        "npm": "6.1.0"
    },
    "scripts": {
        "prestart": "npm run -s build",
        "start": "node dist/app",
        "dev": "nodemon --exec \"babel-node src/app.js\"",
        "build": "babel src --out-dir dist",
        "lint": "eslint -c .eslintrc src",
        "test": "npm run test:all",
        "dev:debug": "nodemon --inspect --exec \"npm run lint && babel-node src/app.js\"",
        "test:all": "mocha --opts ./mocha.opts __test__/e2e/**/**.spec.js",
        "test:auth": "mocha --opts ./mocha.opts __test__/e2e/rest/auth/**.spec.js",
        "test:resource": "mocha --opts ./mocha.opts __test__/e2e/rest/**/resource.spec.js"
    },
    "keywords": [
        "Advance",
        "api",
        "nodejs",
        "express"
    ],
    "author": "Hamid",
    "license": "ISC",
    "dependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/node": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/polyfill": "^7.8.7",
        "@babel/preset-env": "^7.9.6",
        "@babel/register": "^7.9.0",
        "@hapi/joi": "^17.1.1",
        "apollo-errors": "^1.9.0",
        "apollo-resolvers": "^1.4.2",
        "apollo-server-express": "^2.13.0",
        "axios": "^0.18.1",
        "body-parser": "^1.19.0",
        "config": "^3.3.1",
        "dotenv": "^7.0.0",
        "enum": "^2.5.0",
        "env-test": "^1.0.0",
        "express": "^4.17.1",
        "express-http-proxy": "^1.6.0",
        "express-session": "^1.17.1",
        "joi": "^17.2.0",
        "lodash": "^4.17.11",
        "moment": "^2.25.3",
        "moment-timezone": "^0.5.28",
        "mongoose": "^5.9.12",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.6",
        "q": "^1.5.1",
        "query-string": "^6.12.1",
        "request-promise": "^4.2.5",
        "sanitize-html": "^1.26.0",
        "slugify": "^1.4.0",
        "uuid": "^3.4.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "easygraphql-tester": "^5.1.6",
        "eslint": "^7.17.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-mocha": "^4.11.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-watch": "^3.1.3",
        "mocha": "^5.2.0",
        "npm-run-all": "^4.1.2",
        "prettier": "^2.2.1",
        "should": "^13.1.3",
        "supertest": "^3.0.0"
    }
}
